{
  "name": "pixie-extension",
  "displayName": "pixie-extension",
  "description": "Different tools to improve ur coding experience in vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pixie.surpriseColor",
        "title": "Pixie: Surprise me with a Workbench Color"
      },
      {
        "command": "pixie.color",
        "title": "Pixie: Choose a color for ur Workbench"
      },
      {
        "command": "pixie.removeColor",
        "title": "Pixie: Remove color from Workbench"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "color": "^4.2.3",
    "glob": "^10.3.3"
  }
}
